  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yeqown/socket.io-app/issues"
  },
  "homepage": "https://github.com/yeqown/socket.io-app#readme",
  "devDependencies": {
    "@grpc/proto-loader": "^0.5.2",
    "@types/bluebird": "^3.5.27",
    "@types/express": "^4.17.1",
    "@types/google-protobuf": "^3.7.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.4",
    "@types/redis": "^2.8.13",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/uuid": "^3.4.5",
