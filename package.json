{
  "name": "socket.io-app",
  "version": "1.0.0",
  "description": "an app based socket.io",
  "main": "./dist/main.js",
  "scripts": {
    "test-js": "npm run compile && ./node_modules/.bin/mocha ./tests/**",
    "test-ts": "mocha -r ts-node/register tests/**/*.test.ts",
    "compile": "rm -fr ./dist && ./node_modules/.bin/tsc",
